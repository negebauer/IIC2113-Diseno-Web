{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "PORT=8080 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --testResultsProcessor=./node_modules/jest-junit",
    "test:once": "CI=true react-scripts test --env=jsdom",
    "test:coverage": "CI=true react-scripts test --env=jsdom --coverage",
    "test:snap": "CI=true react-scripts test --env=jsdom -u",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src"
  },
  "dependencies": {
    "history": "^4.6.3",
    "lodash": "^4.17.4",
    "popsicle": "^9.1.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.8.3",
    "redux-promise-middleware": "4.2.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.2.1",
    "jest-enzyme": "^3.8.0",
    "jest-junit": "^2.1.0",
    "prettier": "^1.5.3",
    "react-scripts": "1.0.11",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/index.js"
    ]
  },
  "jest-junit": {
    "output": "./coverage/junit.xml",
    "usePathForSuiteName": "true",
    "classNameTemplate": "",
    "titleTemplate": "{title}"
  }
}
